{
  "name": "timeside-player",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:unit:watch": "vue-cli-service test:unit --coverage --watch",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e:headless": "vue-cli-service test:e2e --headless --browser chrome",
    "lint-fix": "vue-cli-service lint",
    "lint": "vue-cli-service lint --no-fix --max-warnings 0"
  },
  "dependencies": {
    "@ircam/timeside-sdk": "^1.2.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-scale": "^2.2.0",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-shape": "^1.3.2",
    "@types/web-animations-js": "^2.2.8",
    "@vue/composition-api": "^0.5.0",
    "browser-update": "^3.3.16",
    "core-js": "^3.6.5",
    "d3-axis": "^1.0.12",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "direct-vuex": "^0.10.4",
    "portable-fetch": "^3.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.6",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.1.3",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-e2e-cypress": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.33",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^3.8.3",
    "vti": "0.0.3",
    "vue-cli-plugin-postcss-preset": "^1.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFiles": [
      "<rootDir>/tests/unit/setup-composition-api.ts",
      "<rootDir>/tests/unit/setup-types-node.ts"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "jest --bail --findRelatedTests"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 2
      }
    }
  },
  "vue": {
    "publicPath": ""
  }
}
